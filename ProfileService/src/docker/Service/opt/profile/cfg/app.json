{
	"Class": "coyote.profile.ProfileService",

	"Transport": {
		"Uri": "amqp://[#$username#]:[#$password#]@[#$hostname#]:[#$port#]/[#$vhost#]",
		"AutoAck": false
	},
		
	"Logging": {
		"StandardOutput": { "categories": "info, notice, warn" },
		"StandardError": { "categories": "error, fatal" },
		"CyclingFileAppender": { "name": "debug", "target": "debug.log", "categories": "trace, debug, info, notice, error, fatal, warn", "generations": 10, "maxsize": "2MB", "enabled": false }
	}

}
